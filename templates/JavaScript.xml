      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="inhere" value="console.log('\n\n\n IN HERE \n\n\n');" description="Console Log 'IN HERE'" toReformat="false" toShortenFQNames="true">
